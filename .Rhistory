################################################################################
N=sum(data_histo$occurence)
json_obj <- list(
labels = as.character(data_histo$classe),
data = data_histo$occurence,
count = N
)
write_json(json_obj, "output/histo_comprime_mdma.json", pretty = TRUE, auto_unbox = FALSE)
