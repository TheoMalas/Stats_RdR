count(classe, name = "occurence") %>%
mutate(classe = as.integer(as.character(classe))) %>%
right_join(tranches, by = "classe") %>%
mutate(occurence = ifelse(is.na(occurence), 0, occurence)) %>%
arrange(classe)
################################################################################
