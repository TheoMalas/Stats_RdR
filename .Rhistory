extract_number_combined <- function(x) {
x_clean <- gsub("\\s+", "", x)  # Supprimer tous les espaces
# Liste des regex à tester
patterns <- c(
".*?(\\d+[\\.,]?\\d*)mgdeMDMA.*",
".*?(\\d+[\\.,]?\\d*)mgMDMA/.*",
".*?(\\d+[\\.,]?\\d*)mgeqbase.*"
)
